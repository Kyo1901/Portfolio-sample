{
  "mcpServers": {
    "netlify": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@netlify/mcp"
      ],
      "env": {
        "NETLIFY_PERSONAL_ACCESS_TOKEN": "your-netlify-token-here"
      }
    },
    "supabase": {
      "type": "stdio",
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--project-ref=your-supabase-project-ref-here"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "your-supabase-access-token-here"
      }
    }
  }
}
